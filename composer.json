{
    "name": "phpshots/event",
    "description": "A simple event manager for PHP with support for listeners and event arguments.",
    "version": "0.1.1",
    "type": "library",
    "autoload": {
        "psr-4": {
            "PHPShots\\Event\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPShots\\Event\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "authors": [
        {
            "name": "Shamavu Rasheed",
            "email": "hakimushamavu@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^11.4"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "license": "MIT",
    "homepage": "https://github.com/your-repo-url", 
    "keywords": [
        "event",
        "event manager",
        "listeners",
        "php"
    ]
